<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Pkg.Admin.maps">

	<resultMap id="AuthMap" type="Pkg.Admin.VO.AuthDTO">
		<result column="AUTHID" property="authID"/>
		<result column="AUTHNAME" property="authName"/>
	</resultMap>
	
	<resultMap id="AuthMemberMap" type="Pkg.Admin.VO.AuthMemberDTO">
		<result column="AUTHID" property="authID"/>
		<result column="AUTHNAME" property="authName"/>
		<result column="MID" property="mID"/>
		<result column="MNAME" property="mName"/>
	</resultMap>	
	
	<resultMap id="ProgramMap" type="Pkg.Admin.VO.ProgramDTO">
		<result column="PID" property="pID"/>
		<result column="PNAME" property="pName"/>
		<result column="MENUID" property="menuID"/>
		<result column="PFILENAME" property="pFileName"/>
		<result column="FILEURL" property="fileUrl"/>
	</resultMap>
	
	<resultMap id="MenuMap" type="Pkg.Admin.VO.AuthMenuDTO">
		<result column="MENUID" property="menuID"/>
		<result column="MENUNAME" property="menuName"/>
		<result column="MENUSEQ" property="menuSeq"/>
		<result column="MENULVL" property="menuLvl"/>
		<result column="PARENTID" property="parentID"/>
	</resultMap>
</mapper>